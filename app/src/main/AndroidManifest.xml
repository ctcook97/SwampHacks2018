<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kyle.studyhack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Colab"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DownOn"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ConfirmOff"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.custom" />
        <activity android:name=".Filter" />
        <activity
            android:name=".classes"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".ContactInfo" />
        <activity android:name=".GroupCreate" />
        <activity android:name=".StartPage"></activity>
    </application>

</manifest>